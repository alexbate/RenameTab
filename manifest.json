{
  "manifest_version": 2,

  "name": "RenameTab",
  "description": "This extension lets you customise the title of any tab.",
  "version": "1.4",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
	"128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.cl.cam.ac.uk/teaching/*"],
      "js": ["cl.js"]
    },
	{
		"matches":
		["*://camtools.cam.ac.uk/access/content/*"],
		"js": ["camtools.js"]
	},
	{
		"matches":
		["*://www.maths.cam.ac.uk/undergradnst/pastpapers//*"],
		"js": ["nstmaths.js"]
	}
  ],
  "permissions": [
    "tabs",
	"http://*/",
	"activeTab"
  ]
  
}