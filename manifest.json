{
  "manifest_version": 2,

  "name": "RenameTab",
  "description": "This extension lets you customise the title of any tab.",
  "version": "0.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
	"http://*/",
	"activeTab"
  ]
  
}