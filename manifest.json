{
  "manifest_version": 2,

  "name": "RenameTab",
  "description": "This extension lets you customise the title of any tab.",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
	"128": "icon.png"
  },
  "content_scripts": [
    {
      "matches":
      ["*://*.cam.ac.uk/*"],
      "js": ["cambridge.js"]
    }
  ],
  "permissions": [
    "tabs",
	"http://*/",
	"activeTab"
  ]
  
}